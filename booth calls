#include <stdio.h>

int main()
{
   int tt;
   float tamt;
   
      printf("Enter total time in seconds:");
      scanf("%d",&tt);
      if(tt>tt+120)
      
     if(tt<=180)
        tamt=1;
     else
        if(tt<=300)
          tamt=1+0.75;
        else
           if(tt<=420)
             tamt=1+0.75+0.75;
             
      
      
      
      
      
      
    printf("You have to pay Rs:%f\n",tamt);
    
    return 0;
}
      
      
      
      
      
